{
    "name": "sagnikrivud/php-folderaspire",
    "description": "Create dynamic folder at storage or any location",
    "homepage": "https://github.com/sagnikrivud/php-folderaspire",
    "type": "library",
    "keywords": ["php", "wordpress"],
    "license": "MIT",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://github.com/sagnikrivud/php-folderaspire"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/console": "^4.0||^5.0||^6.0",
        "backpack/crud": "^5.0",
        "backpack/filemanager": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Sagnikd\\FolderAspire\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Sagnik Dey",
            "email": "sagnikcoold@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
